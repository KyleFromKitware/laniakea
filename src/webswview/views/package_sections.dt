extends base

block head
  - auto title = "Package sections in " ~ currentSuite;

block contents
  h1 Package sections in #{currentSuite}

  .list-group
    - foreach (ref sec; pkgSections)
      a.list-group-item(href='/suites/#{currentSuite}/#{sec.name}/')
        h5.list-group-item-heading #{sec.name}
