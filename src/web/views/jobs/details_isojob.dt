extends jobs/details

block head
    - auto title = "Image build job " ~ job.uuid.toString;

block job_header
    h1 Image build job #{job.uuid.toString}

block pre_log
    h3 Details

    b Distribution:
    |  #{job.data["distribution"].get!string}
    br

    b Suite:
    |  #{job.data["suite"].get!string}
    br

    b Architecture:
    |  #{job.architecture}
    br
